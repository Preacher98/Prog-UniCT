size(640,720);

//Body
rectMode(CENTER);
rect(width/2,(height/2)-80,100,300,50);
//arms
strokeWeight(5);
line((width/2)-50,(height/2)-150,210,height/2);
line((width/2)+50,(height/2)-150,480,height/2);
strokeWeight(1);
//Head
circle(width/2,100,150);
//Eyes
circle((width/2)-30,80,30);
strokeWeight(5);
point((width/2)-30,80);
strokeWeight(1);
circle((width/2)+30,80,30);
strokeWeight(5);
point((width/2)+30,80);
strokeWeight(1);
//Nose
triangle((width/2)-10,110,width/2,90,(width/2)+10,110);
//Mouth
ellipse(width/2,135,60,20);
